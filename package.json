{
  "name": "monday-chat",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run stop && npm run server",
    "build": "react-scripts build",
    "expose": "ngrok http 8301",
    "server": "react-scripts start",
    "eject": "react-scripts eject",
    "stop": "kill-port 8301 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^2.0.0-alpha.0",
    "@giphy/react-components": "^2.0.0-alpha.0",
    "emoji-picker-react": "^3.2.4",
    "monday-sdk-js": "^0.1.1",
    "monday-ui-react-core": "0.0.30",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.0",
    "uuid": "^8.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-port-killer": "^1.2.1",
    "ngrok": "^3.2.7"
  }
}
